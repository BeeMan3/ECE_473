%%Page: (1-2) 1
%%BeginPageSetup
/pagesave save def
sh 0 translate 90 rotate
%%EndPageSetup
iso1dict begin
gsave
llx lly 12 add translate
/v 0 store
/x0 x v get 3.147420 add sx cw mul add store
/y0 y v get bfs th add sub store
x0 y0 moveto
(//UART Example for inclass coding) c n
(//Roger Traylor 12.4.12) N
(//Connect two mega128 boards via rs232 and they should end to each) N
(//other a message and a sequence number.) N
(//) N
(//Change the message you send to your partner for checkoff.) N
(//) N
(//You can test this code by a "loopback" if you connect rx to tx) N
(//on the DB9 connector.) N
(//) N
() p n
(#include) K
( <avr/io.h>) p n
(#include) K
( <stdlib.h>) p n
(#include) K
( <string.h>) p n
(#include) K
( ") p
(uart_functions.h) str
(") p n
(#include) K
( ") p
(hd44780.h) str
(") p n
(#include) K
( ") p
(twi_master.h) str
(") p n
(#include) K
( <avr/interrupt.h>) p n
(#include) K
( <util/delay.h>) p n
() N
(uint8_t SHT21_rd_buf[2];) N
(uint8_t SHT21_wr_buf[2];) N
(uint8_t temp_cmd[2];) S
(// = 0b11100011;) c n
(uint8_t           i;) p n
(volatile) K
( uint8_t  rcv_rdy;) p n
(char) k
(              rx_char; ) p n
(char) k
(              lcd_str_array[16];  ) p
(//holds string to send to lcd) c n
(uint8_t           send_seq=0;         ) p
(//transmit sequence number) c n
(char) k
(              lcd_string[3];      ) p
(//holds value of sequence number) c n
(char) k
(    lcd_string_array[16];  ) p
(//holds a string to refresh the LCD) c n
() p n
(void) k
( spi_init\() p
(void) k
(\){) p n
() S 8 T (DDRB  |= 0x07;           ) S
(//Turn on SS, MOSI, SCLK pins) c n
() p 8 T (SPCR  |= \(1<<SPE\) | \(1<<MSTR\);  ) S
(//set up SPI mode) c n
() p 8 T (SPSR  |= \(1<<SPI2X\);            ) S
(//run at double speed ) c n
(}) p
(//spi_init    ) c n
() p n
(int) k
( main\(\){) p n
() S 8 T () S
(float) k
( temp;) p n
() S 8 T (uint16_t SHT21_temp;) N
() S 8 T () S
(//) c 16 T (temp_cmd[0] = 0xE3;) N
() p 8 T () S
(//) c 16 T (DDRC |= \(1<<PC4\) | \(1<<PC5\);) N
() p 8 T (uart_init\(\);  ) N
() S 8 T () S
(//  spi_init\(\);) c n
() p 8 T (init_twi\(\);) N
() N
() S 8 T (sei\(\);) N
() S 8 T () S
(while) K
(\(1\){) p n
() S 8 T () S 16 T () S
(if) K
(\(rcv_rdy\){) p
(//strlen\(lcd_str_array\)\){) c n
() p 8 T () S 16 T () S 24 T (SHT21_wr_buf[0] = 0xE3;) N
() S 8 T () S 16 T () S 24 T (twi_start_wr\(0b10000001, SHT21_wr_buf, 1 \); ) S
(//read tempe) c n
(rature data from LM73 \(2 bytes\) ) N
() p 8 T () S 16 T () S 24 T (_delay_ms\(2\);    ) S
(//wait for it to finish) c n
() p 8 T () S 16 T () S 24 T (twi_start_rd\(0b10000001, SHT21_rd_buf, 2\); ) S
(//read temper) c n
(ature data from LM73 \(2 bytes\) ) N
() p 8 T () S 16 T () S 24 T (_delay_ms\(2\);    ) S
